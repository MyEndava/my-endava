<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/primary" />
            <corners android:radius="@dimen/calendar_switch_track_selector_radius" />
            <size android:width="@dimen/calendar_switch_track_selector_width" android:height="@dimen/calendar_switch_track_selector_height" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">

            <corners android:radius="@dimen/calendar_switch_track_selector_radius" />
            <size android:width="@dimen/calendar_switch_track_selector_width" android:height="@dimen/calendar_switch_track_selector_height" />
            <stroke android:width="@dimen/calendar_switch_track_selector_stroke_width" android:color="@color/white" />
        </shape>
    </item>
</selector>